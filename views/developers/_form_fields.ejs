<div class="form-row">
    <div class="form-item">
    <label>Name</label>
    <input type="text" name="name" value="<%= developer.name %>">
    </div>
</div>

<div class="form-row">
    <div class="form-item">
    <label>Location</label>
    <input type="text" name="location" value="<%= developer.location %>">
    </div>
    <div class="form-item">
        <label>Established (Year)</label>
        <input type="number" name="established" min="1800" value="<%= developer.established %>">
        </div>
</div>
